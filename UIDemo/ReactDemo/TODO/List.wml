<ws:if data="{{_options.items.length}}">
   <div className="todo-List">
      <ws:for data="i in _options.items">
         <UIDemo.ReactDemo.TODO.Item
               key="{{i.id}}"
               title="{{i.title}}"
               removeHandler="{{_options.removeHandler.bind(undefined, i.id)}}"/>
      </ws:for>
   </div>
</ws:if>
<ws:else>
   <div>
      Список пуст
   </div>
</ws:else>