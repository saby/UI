<div style="color:{{_options.color}};">
    <div>{{_options.count}}</div>
    <button on:click="_options.addHandler()">Add counter</button>
    <button on:click="_options.removeHandler()">Minus counter</button>
    <Controls.buttons:Button name="Btn" on:click="_options.changeColor()" caption="ChangeColor without rerender"
                             theme="default"/>
</div>
