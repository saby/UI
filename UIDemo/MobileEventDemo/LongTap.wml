<div class="container">
    <div>tapCount: <span>{{ tapCount }}</span></div>
    <div>swipeCount:  <span>{{ swipeCount }}</span></div>
    <div>longTapCount:  <span>{{ longTapCount }}</span></div>
    <div on:longtap="longClick()" on:swipe="swipe()" class="lcontainer__button"></div>
    <Controls.heading:Title caption="longTapOnly" readOnly="{{true}}" fontColorStyle="primary" fontSize="l"  on:longtap="longClick()"/>
    <Controls.buttons:Button on:click="simpleClick()" caption="tapOnly" class="container__button"/>
    <Controls.buttons:Button on:longtap="longClick()" caption="longTapOnly" class="container__button"/>
</div>
